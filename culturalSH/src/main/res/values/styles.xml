<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 日程 -->
    <style name="LeaderScheduleScheduleTtileText">
        <item name="android:textColor">@color/translucent</item>
        <item name="android:textSize">@dimen/leader_schedule_Schedule_TtileText_size</item>
        <item name="android:singleLine">false</item>
        <item name="android:ellipsize">end</item>
        <item name="android:padding">@dimen/leader_schedule_Schedule_TtileText_paddingSpacing</item>
        <item name="android:gravity">center</item>
        <item name="android:ems">1</item>
        <item name="android:layout_weight">7</item>
    </style>
    <style name="Animation.Toast" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/toast_enter</item>
        <item name="android:windowExitAnimation">@anim/toast_exit</item>
    </style>

    <declare-styleable name="CustomTheme">
        <attr name="gifViewStyle" format="reference" />
    </declare-styleable>

    <style name="LeaderScheduleScheduleDetailText">
        <item name="android:textColor">@color/translucent</item>
        <item name="android:textSize">@dimen/leader_schedule_Schedule_DetailText_size</item>
        <item name="android:singleLine">false</item>
        <item name="android:ellipsize">end</item>
        <item name="android:padding">@dimen/leader_schedule_Schedule_DetailText_paddingSpacing</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CalendarTitleText">

        <!-- 日期 2013年8月8日 -->
        <item name="android:textColor">@color/list_item_view_95_white</item>
        <item name="android:textSize">@dimen/leader_schedule_Schedule_Calendar_text_size</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="CalendarWeekDayText">

        <!-- 日历 星期 -->
        <item name="android:textColor">@color/text_color_26</item>
        <item name="android:textSize">@dimen/text_size_26px</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:paddingLeft">@dimen/calendar_week_day_item_marginLeft</item>
        <item name="android:paddingRight">@dimen/calendar_week_day_item_marginRight</item>
        <item name="android:paddingTop">@dimen/calendar_week_day_item_marginTop</item>
        <item name="android:paddingBottom">@dimen/calendar_week_day_item_marginBottom</item>
    </style>

    <style name="CalendarItemDayText">

        <!-- 日历Item日字体样式 -->
        <item name="android:textColor">@color/text_color_26</item>
        <item name="android:textSize">@dimen/text_size_32px</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center</item>
