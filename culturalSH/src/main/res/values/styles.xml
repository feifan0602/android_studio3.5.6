        <item name="android:background">@android:color/transparent</item>
        <item name="android:backgroundDimAmount">0.6</item>
    </style>

    <style name="DuoappNormal" parent="@android:style/Theme.Light">
        <item name="android:windowBackground">@color/activity_bg_color</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:fadingEdge">none</item>
        <!-- 去掉顶部的渐变 -->
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">false</item>
    </style>

    <style name="MyAPPTheme" parent="@android:style/Theme.Light">
        <item name="android:windowBackground">@drawable/sh_icon_app_splash</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:fadingEdge">none</item>
        <!-- 去掉顶部的渐变 -->
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">false</item>
    </style>
